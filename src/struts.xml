<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default">

		<!--Actions para registros de servicios de informaciÃ³n -->
		<action name="prepararServicioInformacion" method="prepararFormulario"
			class="ve.gob.cnti.srsi.controlador.ServicioInformacionControlador">
			<result name="success">registro/registro_servicio_informacion.jsp
			</result>
		</action>

		<action name="registrarServicioInformacion" method="registrarServicioInformacion"
			class="ve.gob.cnti.srsi.controlador.ServicioInformacionControlador">
			<result name="input">registro/registro_servicio_informacion.jsp
			</result>
			<result name="success">registro/servicio_informacion.jsp</result>
		</action>

		<action name="registrarPrueba" method="registrarPrueba"
			class="ve.gob.cnti.srsi.controlador.ServicioInformacionControlador">
			<result name="success">registro/servicio_informacion.jsp</result>
		</action>

		<!--Actions para Funcionalidades -->
		<action name="prepararFuncionalidad" method="prepararFormulario"
			class="ve.gob.cnti.srsi.controlador.FuncionalidadControlador">
			<result name="success">registro/registro_funcionalidad.jsp</result>
		</action>

		<action name="registrarFuncionalidad" method="registrarFuncionalidad"
			class="ve.gob.cnti.srsi.controlador.FuncionalidadControlador">

			<result name="success" type="chain">
				<param name="actionName">prepararEntradas</param>
			</result>

			<result name="input">registro/registro_funcionalidad.jsp</result>
		</action>

		<!--*****************************************************************
						Actions para Entradas 
		********************************************************************-->
		<action name="prepararEntradas" method="prepararFormulario"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">
			<result name="success">registro/entradas.jsp</result>
		</action>

		<action name="prepararEntradaSimple" method="prepararFormularioSimple"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">
			<result name="success">registro/registro_entrada_simple.jsp</result>
		</action>
		
		<action name="registrarEntradaSimple" method="registrarEntradaSimple"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">
			<result name="success" type="chain">
               <param name="actionName">prepararEntradas</param>                         
            </result>            
			<result name="input">registro/registro_entrada_simple.jsp</result>
		</action>

		<action name="prepararEntradaCompleja" method="prepararFormularioComplejo"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">
			<result name="success">registro/registro_entrada_compleja.jsp</result>
		</action>


		<action name="registrarEntradaSimple" method="registrarEntradaSimple"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">

			<result name="success" type="chain">
				<param name="actionName">prepararEntradas</param>
			</result>

			<result name="input">registro/registro_entrada_simple.jsp</result>
		</action>

		<action name="registrarEntradaCompleja" method="registrarEntradaCompleja"
			class="ve.gob.cnti.srsi.controlador.EntradaControlador">
			<result name="success" type="chain">
               <param name="actionName">prepararEntradas</param>                         
            </result>            
			<result name="input">registro/registro_entrada_compleja.jsp</result>
		</action>	
		
		<!--*****************************************************************
						Actions para Salidas
		********************************************************************-->
		<action name="prepararSalidas" method="prepararFormulario"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">
			<result name="success">registro/salidas.jsp</result>
		</action>
		
		<action name="prepararSalidaSimple" method="prepararFormularioSimple"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">
			<result name="success">registro/registro_salida_simple.jsp</result>
		</action>
		
		<action name="registrarSalidaSimple" method="registrarSalidaSimple"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">
			<result name="success" type="chain">
               <param name="actionName">prepararSalidas</param>                         
            </result>            
			<result name="input">registro/registro_salida_simple.jsp</result>
		</action>

		<action name="prepararSalidaCompleja" method="prepararFormularioComplejo"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">
			<result name="success">registro/registro_salida_compleja.jsp</result>
		</action>


		<action name="registrarSalidaSimple" method="registrarSalidaSimple"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">

			<result name="success" type="chain">
				<param name="actionName">prepararSalidas</param>
			</result>

			<result name="input">registro/registro_salida_simple.jsp</result>
		</action>

		<action name="registrarSalidaCompleja" method="registrarSalidaCompleja"
			class="ve.gob.cnti.srsi.controlador.SalidaControlador">
			<result name="success" type="chain">
               <param name="actionName">prepararSalidas</param>                         
            </result>            
			<result name="input">registro/registro_salida_compleja.jsp</result>
		</action>	
		
	</package>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
</struts>